{
    "name": "Emotionally Mute VSCode",
    "type": "light",
    "colors": {
        "activityBar.background": "#333",
        "activityBar.foreground": "#EEE",
        "activityBarBadge.background": "#d44a78",
        "button.background": "#d44a78",
        "button.foreground": "#CCC",
        "editor.background": "#DEDEDE",
        "editor.foreground": "#333",
        "editor.lineHighlightBackground": "#EEE",
        "editor.lineHighlightBorder": null,
        "editorLineNumber.foreground": "#AAA",
        "editorLineNumber.activeForeground": "#333",
        "editorWhitespace.foreground": "#CDCDCD",
        "editorGutter.background": "#DEDEDE",
        "editor.selectionBackground": "#fffec3",
        "editor.selectionHighlightBackground": "#fffec3",
        "editor.findMatchHighlightBackground": "#fffec3",
        "editor.findMatchBackground": "#FFF",
        "editorBracketMatch.background": "#fffec3",
        "editorBracketMatch.border": "#fffec3",
        "peekViewEditor.background": "#EEE",
        "peekViewTitle.background": "#CCC",
        "peekView.border": "#555",
        "peekViewResult.selectionBackground": "#666",
        "peekViewResult.selectionForeground": "#CCC",
        "peekViewResult.background": "#DDD",
        "sideBar.background": "#CCC",
        "sideBar.foreground": "#111",
        "statusBar.background": "#333",
        "statusBar.foreground": "#CCCCCC",
        "tab.border": "#FFF",
        "tab.activeBackground": "#FFF",
        "tab.activeForeground": "#666",
        "tab.inactiveBackground": "#AAA",
        "tab.inactiveForeground": "#333",
        "terminal.ansiYellow": "#a56d2c",
        "terminalCursor.foreground": "#333"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#AAA",
                "background": "#FFF"
            }
        },
        {
            "name": "String - Quoted",
            "scope": [
                "string.quoted"
            ],
            "settings": {
                "foreground": "#01a252"
            }
        },
        {
            "name": "String - Regexp",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#37723f",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Entity Tags",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#777"
            }
        },
        {
            "name": "Entity Tags, Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#37723f"
            }
        },
        {
            "name": "Storage Types",
            "scope": [
                "storage.type",
                "storage.modifier",
                "variable.function",
                "keyword.other.var",
                "keyword.type",
                "keyword.control.at-rule.import"
            ],
            "settings": {
                "foreground": "#666",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variable types",
            "scope": [
                "punctuation.definition.variable",
                "variable.language",
                "variable.other",
                "variable.parameter",
                "entity.name.variable.parameter",
                "support.variable.property.dom",
                "entity.name.variable.local",
                "variable.other.object.property",
                "entity.name.variable.field"
            ],
            "settings": {
                "foreground": "#478ed1"
            }
        },
        {
            "name": "Variable props",
            "scope": [
                "entity.name.function",
                "support.function.dom"
            ],
            "settings": {
                "foreground": "#346592"
            }
        },
        {
            "name": "Class names",
            "scope": [
                "entity.name.type.class"
            ],
            "settings": {
                "foreground": "#478ed1"
            }
        }
    ]
}